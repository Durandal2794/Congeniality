CREATE TABLE HGNC (
	HGNC_ID VARCHAR(500) PRIMARY KEY,
	Approved_Symbol VARCHAR(500),
	Approved_Name VARCHAR(500),
	Status VARCHAR(500),
	Locus_Type VARCHAR(500),
	Locus_Group VARCHAR(500),
	Previous_Symbols VARCHAR(500),
	Previous_Names VARCHAR(500),
	Synonyms VARCHAR(500),
	Name_Synonyms VARCHAR(500),
	Chromosome VARCHAR(500),
	Date_Approved DATE,
	Date_Modified DATE,
	Date_Symbol_Changed DATE,
	Date_Name_Changed DATE,
	Accession_Numbers VARCHAR(500),
	Enzyme_IDs VARCHAR(500),
	Entrez_Gene_ID VARCHAR(500),
	Ensemble_Gene_ID VARCHAR(500),
	Mouse_Genome_Database_ID VARCHAR(500),
	Specialist_Database_Links VARCHAR(5000),
	Specialist_Database_IDs VARCHAR(500),
	PubMed_IDs VARCHAR(500),
	RefSeq_IDs VARCHAR(500),
	Gene_Family_Tag VARCHAR(500),
	Gene_Family_Description VARCHAR(500),
	Record_Type VARCHAR(500),
	Primary_IDs VARCHAR(500),
	Secondary_IDs VARCHAR(500),
	CCDS_IDs VARCHAR(500),
	VEGA_IDs VARCHAR(500),
	Locus_Specific_Databases VARCHAR(500),
	Entrez_Gene_ID_Supplied_by_NCBI VARCHAR(500),
	OMIM_ID_Supplied_by_NCBI VARCHAR(500),
	RefSeq_Supplied_by_NCBI VARCHAR(500),
	UniProt_ID_Supplied_by_UniProt VARCHAR(500),
	Ensembl_ID_Supplied_by_Ensembl VARCHAR(500),
	UCSC_ID_Supplied_by_UCSC VARCHAR(500),
	Mouse_Genome_Database_ID_Supplied_by_MGI VARCHAR(500),
	Rat_Genome_Database_ID_Supplied_by_RGD VARCHAR(500)
) ENGINE=InnoDB;


LOAD DATA LOCAL INFILE "/Users/christophercotton/Documents/Tufts/Spring 2015/Practicum/hgnc_complete_set.txt"
INTO TABLE HGNC
FIELDS TERMINATED BY '\t';


UPDATE HGNC
SET HGNC_ID = REPLACE (HGNC_ID, 'HGNC:', '');


ALTER TABLE HGNC MODIFY HGNC_ID INTEGER;

ALTER TABLE HGNC ADD cdsStart INTEGER AFTER Chromosome;
ALTER TABLE HGNC ADD cdsEnd INTEGER AFTER cdsStart;

UPDATE HGNC
SET Approved_Symbol = REPLACE (Approved_Symbol, '~withdrawn', '');
