CREATE TABLE ProteinAtlas (
	Gene VARCHAR(500),
	Gene_synonym VARCHAR(500),
	Ensembl VARCHAR(500),
	Gene_description VARCHAR(500),
	Chromosome VARCHAR(500),
	Position VARCHAR(500),
	Protein_class VARCHAR(500),
	Evidence_summary VARCHAR(500),
	HPA_evidence VARCHAR(500),
	UniProt_evidence VARCHAR(500),
	MS_evidence VARCHAR(500),
	Antibody VARCHAR(500),
	Reliability_IH VARCHAR(500),
	Reliability_IF VARCHAR(500),
	Subcellular_location VARCHAR(500),
	RNA_tissue_category VARCHAR(500),
	RNA_TS VARCHAR(500),
	RNA_TS_FPKM VARCHAR(500),
	FPKM_max_in_nonspecific VARCHAR(500)
) ENGINE=InnoDB;

LOAD DATA LOCAL INFILE "/Users/christophercotton/Desktop/proteinatlas.txt" INTO TABLE ProteinAtlas 
FIELDS TERMINATED BY '\t';
